@model Piranha.Models.PostModel
@{  
    ViewBag.Title = Model.Post.Title;
}
<div class="content">
    <div class="main">
        <h1>@Model.Post.Title</h1>
        <p class="ingress">@Model.Post.Excerpt</p>
        @Model.Post.Body
    </div>
    <div class="sidebar">
        @Html.Partial("Sidebar")
    </div>
</div>
